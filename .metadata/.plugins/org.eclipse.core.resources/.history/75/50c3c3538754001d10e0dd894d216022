		db.dbOpen();
		
		String sql = "SELECT password FROM user WHERE id = ?";
		
		
		
		db.Close();
		//====================================
	    
		if(lgm.getId().equals("sna145812") && lgm.getpwd().equals("1234"))
		{
			return 1;
		}
		
		
		
